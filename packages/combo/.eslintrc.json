{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*", "node_modules"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.json"],
      "parser": "jsonc-eslint-parser",
      "rules": {
        "@nx/dependency-checks": [
          "error",
          {
            "ignoredDependencies": [
              "jest-fetch-mock",
              "statsig-test-helpers",
              "@nx/webpack",
              "webpack-bundle-analyzer",
              "typescript",
              "@testing-library/react",
              "react"
            ]
          }
        ]
      }
    }
  ]
}
